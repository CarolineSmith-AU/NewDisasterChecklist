        <activity android:name="AdapterRecyclerViewAdditionals.AdditionalsActivity" />
        <activity android:name="AdapterRecyclerViewAdditionals.AdditionalsRecyclerViewActivity" />
        <activity android:name="AddYourOwnItems.AddYourOwnActivity" />
